						<div class="tg-ticket">
									<time class="tg-matchdate" datetime="2016-05-03">@tkm.RandevuBaslangic.Day<span>@tkm.RandevuBaslangic.ToString("MMMM")</span></time>
									<div class="tg-matchdetail">
										<span class="tg-theme-tag">@tkm.Kullanici.Isim @tkm.Kullanici.Soyisim</span>
										<h4>@tkm.Saha.Baslik</h4>
										<ul class="tg-matchmetadata">											
										<li><address><i class="far fa-calendar-alt"></i> @tkm.RandevuBaslangic.ToString("dd.MM.yyyy HH:ss") - <i class="fas fa-calendar-alt"></i> @tkm.RandevuBitis.ToString("dd.MM.yyyy HH:ss")</address></li>
										</ul>
									</div>
									<div class="tg-btnsbox">
												<a class="tg-btn" href="~/Profil/RezervasyonSil/@tkm.Id">Sil</a>
									</div>
								</div>
					}
								</div>
							</div>
	                @Html.PagedListPager((IPagedList)Model,page=>Url.Action("Randevular", new { page }),
                    new X.PagedList.Web.Common.PagedListRenderOptions()
                    {
                    LiElementClasses = new List<string> {"", ""},
					PreviousElementClass="tg-prevpage",
					ActiveLiElementClass="active",
					NextElementClass="tg-nextpage",
                    UlElementClasses = new List<string> {"tg-pagination"},
                    LinkToPreviousPageFormat="<i class=\"fa fa-angle-left\"></i>",
                    LinkToNextPageFormat="<i class=\"fa fa-angle-right\"></i>"
